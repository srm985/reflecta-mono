{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "noUnusedParameters": false,
        "outDir": "./dist"
    },
    "exclude": [
        "./dist/**/*"
    ],
    "extends": "../../tsconfig.json",
    "include": [
        "src/**/*",
        "src/*",
        "./*.js"
    ],
    "ts-node": {
        "esm": true,
        "experimentalSpecifierResolution": "node"
    }
}
