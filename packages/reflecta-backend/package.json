{
  "author": "Sean McQuay",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/uuid": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "description": "Journaling application backend meant to help offer analysis and insights into journal entries.",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "node-gyp": "^9.4.0",
    "nodemon": "^2.0.22",
    "onchange": "^7.1.0",
    "sequelize": "^6.32.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "main": "index.js",
  "name": "reflecta-backend",
  "scripts": {
    "build": "tsc && tsc-alias",
    "config:local": "cross-env NODE_ENV=development",
    "develop": "npm run config:local concurrently --raw \"nodemon\" \"npm:watch --silent\"",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "pre-commit": "cross-env FORCE_COLOR=true lint-staged",
    "start": "npm run develop",
    "update": "npm update --save/--save-dev",
    "watch": "onchange --delay 500 \"src/**/*.(ts|tsx)\" -- npm run lint --silent"
  },
  "version": "1.0.0"
}
