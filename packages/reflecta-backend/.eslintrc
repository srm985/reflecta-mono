{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "../../.eslintrc.js"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "import/order": [
            "error",
            {
                "alphabetize": {
                    "caseInsensitive": true,
                    "order": "asc"
                },
                "groups": [
                    "external",
                    "builtin",
                    "parent"
                ],
                "newlines-between": "always",
                "pathGroups": [
                    {
                        "group": "parent",
                        "pattern": "./controllers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../controllers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../controllers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../controllers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../../controllers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./models/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../models/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../models/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../models/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../../models/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../../services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../../utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./middleware",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./logger",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./db",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./Auth",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../Auth",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../Auth",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../Auth",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./RateLimiter",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../RateLimiter",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../RateLimiter",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../RateLimiter",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./routes/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./constants",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../constants",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../constants",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../constants",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "../../../../constants",
                        "position": "after"
                    }
                ]
            }
        ]
    }
}
