{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript",
        "../../.eslintrc.js"
    ],
    "overrides": [
        {
            "files": [
                "./*.js"
            ],
            "rules": {
                "global-require": [
                    0
                ],
                "import/no-extraneous-dependencies": [
                    0
                ]
            }
        }
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
        "class-methods-use-this": [
            0
        ],
        "import/order": [
            "error",
            {
                "alphabetize": {
                    "caseInsensitive": true,
                    "order": "asc",
                    "orderImportKind": "asc"
                },
                "groups": [
                    "external",
                    "builtin",
                    "parent"
                ],
                "newlines-between": "always",
                "pathGroups": [
                    {
                        "group": "parent",
                        "pattern": "@views/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@views/**/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@modules/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@components/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@components/remotes/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@components/enhancers/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@services/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@store",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@store/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@utils/*",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@routes",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@constants",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./withReducer",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./slice",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@types",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "./types",
                        "position": "after"
                    },
                    {
                        "group": "parent",
                        "pattern": "@assets/*",
                        "position": "after"
                    }
                ],
                "pathGroupsExcludedImportTypes": [
                    "builtin"
                ],
                "warnOnUnassignedImports": true
            }
        ],
        "react/function-component-definition": [
            "error",
            {
                "namedComponents": "arrow-function",
                "unnamedComponents": "arrow-function"
            }
        ],
        "react/jsx-curly-brace-presence": [
            "error",
            {
                "children": "ignore",
                "props": "always"
            }
        ],
        "react/jsx-filename-extension": [
            "error",
            {
                "extensions": [
                    ".ts",
                    ".tsx"
                ]
            }
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "react/jsx-max-props-per-line": [
            "error",
            {
                "maximum": 1
            }
        ],
        "react/jsx-one-expression-per-line": [
            0
        ],
        "react/jsx-props-no-spreading": [
            0
        ],
        "react/jsx-sort-props": [
            "error",
            {
                "ignoreCase": true
            }
        ],
        "react/jsx-uses-react": "off",
        "react/no-did-update-set-state": [
            0
        ],
        "react/no-unescaped-entities": [
            0
        ],
        "react/prop-types": [
            0
        ],
        "react/react-in-jsx-scope": "off"
    }
}
